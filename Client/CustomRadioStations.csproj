<Project Sdk="CitizenFX.Sdk.Client/0.2.3">
	<PropertyGroup>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
		<Version>1.1.3</Version>
	</PropertyGroup>
	<ItemGroup>
		<Compile Remove="Model\SoundHelperIK.cs" />
	</ItemGroup>
	<ItemGroup>
	  <None Remove="wwwroot\radios\dnb\dnb.png" />
	</ItemGroup>
	<ItemGroup>
		<Content Include="wwwroot\radios\dnb\dnb.png" />
		<Content Include="wwwroot\radios\dnb\dnbradio.png" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="12.0.2" ExcludeAssets="Compile" GeneratePathProperty="true" />
		<Reference Include="Newtonsoft.Json">
			<HintPath>$(PkgNewtonsoft_Json)\lib\portable-net40+sl5+win8+wp8+wpa81\Newtonsoft.Json.dll</HintPath>
		</Reference>
	</ItemGroup>
	<ItemGroup>
		<None Update="wwwroot\radios\categories.json">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Update="wwwroot\radios\settings.ini">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>
</Project>
